    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/swot#hasInputDataSchema -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/swot#hasInputDataSchema">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/swot#hasDataSchema"/>
        <owl:inverseOf rdf:resource="http://purl.org/ontology/swot#isInputDataSchemaOf"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/swot#Action"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/swot#DataSchema"/>
        <rdfs:comment>Objectproperty connecting the dataschema expected as input of an action</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/swot#hasInstance -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/swot#hasInstance">
        <owl:inverseOf rdf:resource="http://purl.org/ontology/swot#instanceOf"/>
