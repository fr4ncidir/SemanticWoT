                    <rdf:Description rdf:about="http://purl.org/ontology/swot#Event"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/ontology/swot#ActionInstance"/>
                    <rdf:Description rdf:about="http://purl.org/ontology/swot#EventInstance"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>Generic objectproperty connecting the interaction pattern to its instance</rdfs:comment>
    </owl:ObjectProperty>
    


