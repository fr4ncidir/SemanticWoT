        <rdfs:range rdf:resource="http://purl.org/ontology/swot#InteractionPattern"/>
        <rdfs:comment>Generic objectproperty connecting the thing description to all the interaction patterns belonging to the thing</rdfs:comment>
    </owl:ObjectProperty>
    


