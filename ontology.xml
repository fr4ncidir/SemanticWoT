    <!-- http://purl.org/ontology/swot#ActionInstanceOf -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/swot#ActionInstanceOf">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/swot#instanceOf"/>
        <owl:inverseOf rdf:resource="http://purl.org/ontology/swot#hasActionInstance"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/swot#ActionInstance"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/swot#Action"/>
        <rdfs:comment>Defines which is the Action that originates this ActionInstance</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/swot#EventInstanceOf -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/swot#EventInstanceOf">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/swot#instanceOf"/>
        <owl:inverseOf rdf:resource="http://purl.org/ontology/swot#hasEventInstance"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/swot#EventInstance"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/swot#Event"/>
        <rdfs:comment>Defines which is the Event that originates this EventInstance</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/swot#forProperty -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/swot#forProperty">
        <owl:inverseOf rdf:resource="http://purl.org/ontology/swot#isTargetPropertyFor"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/ontology/swot#Action"/>
                    <rdf:Description rdf:about="http://purl.org/ontology/swot#Event"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://purl.org/ontology/swot#Property"/>
        <rdfs:comment>This objectproperty identifies the Property target of actions and events.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/swot#hasAction -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/swot#hasAction">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/swot#hasInteractionPattern"/>
        <owl:inverseOf rdf:resource="http://purl.org/ontology/swot#isActionFor"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/swot#ThingDescription"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/swot#Action"/>
        <rdfs:comment>Object property connecting the thing description to the interaction pattern Action</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/swot#hasActionInstance -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/swot#hasActionInstance">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/swot#hasInstance"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/swot#Action"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/swot#ActionInstance"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/ontology/swot#hasEventInstance"/>
        <rdfs:comment>Objectproperty connecting the action to its instance</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/swot#hasData -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/swot#hasData">
        <owl:inverseOf rdf:resource="http://purl.org/ontology/swot#isDataFor"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/swot#Instance"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/swot#Data"/>
        <rdfs:comment>Generic objectproperty for connecting data to instances or properties.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/swot#hasDataSchema -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/swot#hasDataSchema">
        <owl:inverseOf rdf:resource="http://purl.org/ontology/swot#isDataSchemaOf"/>
