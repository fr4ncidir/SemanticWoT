    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/swot#instanceOf -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/swot#instanceOf">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/ontology/swot#ActionInstance"/>
                    <rdf:Description rdf:about="http://purl.org/ontology/swot#EventInstance"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/ontology/swot#Action"/>
                    <rdf:Description rdf:about="http://purl.org/ontology/swot#Event"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>Defines which is the Interaction Pattern that originates this Instance</rdfs:comment>
    </owl:ObjectProperty>
    


