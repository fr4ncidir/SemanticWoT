        <rdfs:range rdf:resource="http://purl.org/ontology/swot#EventInstance"/>
        <rdfs:comment>Objectproperty connecting the event to its instance</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/swot#hasFieldSchema -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/swot#hasFieldSchema">
        <owl:inverseOf rdf:resource="http://purl.org/ontology/swot#isFieldSchemaFor"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/swot#DataSchema"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/swot#FieldSchema"/>
        <rdfs:comment>Object property connecting the dataschema to (some) fieldschemas</rdfs:comment>
