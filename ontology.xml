    <!-- http://purl.org/ontology/swot#hasSubThing -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/swot#hasSubThing">
        <owl:inverseOf rdf:resource="http://purl.org/ontology/swot#hasSuperThing"/>
