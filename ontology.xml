            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/ontology/swot#Data"/>
                    <rdf:Description rdf:about="http://purl.org/ontology/swot#InteractionPattern"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://purl.org/ontology/swot#DataSchema"/>
        <rdfs:comment>Generic property connecting data or interaction patterns to their dataschema</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/swot#hasEvent -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/swot#hasEvent">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/swot#hasInteractionPattern"/>
        <owl:inverseOf rdf:resource="http://purl.org/ontology/swot#isEventFor"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/swot#ThingDescription"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/swot#Event"/>
        <rdfs:comment>Object property connecting the thing description to the interaction pattern Event</rdfs:comment>
    </owl:ObjectProperty>
    


